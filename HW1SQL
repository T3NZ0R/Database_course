show databases ;

use t3nz0r;

show tables ;

select * from cars;

select * from cars where year < 2000;
select * from cars where year > 2015;
select * from cars where year in (2008,  2009,  2010);
select * from cars where year != 2008 and year != 2009 and year != 2010;
select * from cars where year = price;

select * from cars where year<2014 and model like '%bmw%';
select * from cars where year>2014 and model like '%audi%';


select * from cars order by id limit 5;
select * from cars order by id desc limit 5;

select avg(price) as avgKIA from cars where model like '%kia%';
select avg(price) as avgAll from cars;

select count(model) as number, model from cars group by model order by number;
select count(model) as number, model from cars group by model order by number desc limit 1;

select * from cars where model like '_a%' or model like '%a_';
select * from cars where model like '%_________%';

select model, price from cars where price > (select avg(price) from cars) ;